<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
  <PropertyGroup>
    <TargetFrameworks>netstandard1.0;net40</TargetFrameworks>
    <PackageVersion>1.0.0</PackageVersion>
    <AssemblyVersion>$(PackageVersion)</AssemblyVersion>
    <Authors>steinpilz,ivan.benko</Authors>
    <Description>Provides abstractions to manage application configuration</Description>
    <PackageTags>.net configuration abstractions confifu</PackageTags>
    <PackageProjectUrl>https://github.com/steinpilz/confifu</PackageProjectUrl>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="**\*.cs" />
    <EmbeddedResource Include="**\*.resx" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="NETStandard.Library" Version="1.6.1" />
  </ItemGroup>
</Project>